@model PlayEv.WebUI.Models.UserLoginViewModel

@{
    ViewBag.Title = "Login";
}
@{
    var message = TempData["message"];
    if(message != null){
        <div>@message</div>
    }
}
<h3>Login</h3>
@using (Html.BeginForm())
{
    @Html.EditorForModel(Model)
    <input type="submit" value="Login" />
}
